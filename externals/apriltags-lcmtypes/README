Caffe Settings in ~/.bashrc
# caffe
export CAFFE_ROOT=$HOME/caffe
export LD_LIBRARY_PATH=$CAFFE_ROOT/build/lib:$LD_LIBRARY_PATH
export PYTHONPATH=$CAFFE_ROOT/python:$HOME/matlab_ws:$PYTHONPATH
export CAFFE_DATA=$HOME/caffe
